      if (checked.length === 0) {
        addBotMessage('Por favor selecciona al menos un desaf√≠o.');
        return;
      }
      
      const challenges = Array.from(checked).map(cb => cb.value);
      conversationState.currentChallenges = challenges;
      conversationState.answers.challenges = challenges;
      
      const challengeTexts = Array.from(checked).map(cb => 
        cb.parentElement.querySelector('.font-semibold').textContent
      );
      addUserMessage(challengeTexts.join(', '));
      showTypingIndicator();
      
      setTimeout(() => {
        hideTypingIndicator();
        proceedToStep(nextStep);
      }, 1500);
