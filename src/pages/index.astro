    window.submitForm = function(nextStep) {
      const inputs = inputArea.querySelectorAll('input, select');
      const formData = {};
      let isValid = true;
      
      inputs.forEach(input => {
        if (input.required && !input.value.trim()) {
          isValid = false;
          input.style.borderColor = '#ff6b6b';
          return;
        }
        input.style.borderColor = '';
        formData[input.name] = input.value;
      });
      
      if (!isValid) {
        addBotMessage('Por favor completa todos los campos requeridos.');
        return;
      }
      
      Object.assign(conversationState.answers, formData);
      
      const summaryText = Object.values(formData).join(', ');
      addUserMessage(summaryText);
      showTypingIndicator();
      
      setTimeout(() => {
        hideTypingIndicator();
        if (nextStep === 'results') {
          generateDiagnosticResults();
        } else {
          proceedToStep(nextStep);
        }
      }, 1500);
    };
    
    window.submitMultipleChoice = function(nextStep) {
