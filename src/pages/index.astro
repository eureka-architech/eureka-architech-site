      }
      
      return recommendations.slice(0, 3);
    }
    
    function calculateImpactMetrics(state) {
      const { mainGoal, challenges, budget } = state.answers;
      
      let efficiency = 25;
      let cost = 15;
      let growth = 20;
      
      // Adjust based on main goal
      if (mainGoal === 'growth') {
        growth += 15;
        efficiency += 10;
      } else if (mainGoal === 'efficiency') {
        efficiency += 20;
        cost += 10;
      }
      
      // Adjust based on challenges
      if (challenges.includes('digital-transformation')) {
        efficiency += 15;
        growth += 10;
      }
      if (challenges.includes('process-optimization')) {
        efficiency += 10;
        cost += 15;
      }
      
      return {
        efficiency: Math.min(efficiency, 60),
        cost: Math.min(cost, 45),
        growth: Math.min(growth, 50)
      };
    }
    
