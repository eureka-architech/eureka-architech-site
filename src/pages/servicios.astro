  <!-- Hero Section - Servicios -->
  <section class="py-24 bg-gradient-to-br from-primary-900 to-primary-800">
    <div class="container text-center">
      <h1 class="text-display text-primary mb-6">
        Servicios de <span class="text-accent">Consultoria</span>
